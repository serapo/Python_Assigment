numbers = [1, 3, 7, 4, 3, 0, 3, 6, 3]
max = 0
res = numbers[0]
for i in numbers:
     x= numbers.count(i)
if x > max:
      max = x
      res = i
     
# printing result

repeat_times =numbers.count(res) 

print("The most frequent number is {} and it was {} times repeated" .format(res,repeat_times))
